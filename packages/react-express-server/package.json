{
  "name": "react-express-server",
  "version": "2.0.4",
  "description": "An opinionated express server to render React apps",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rimraf dist es lib",
    "prebuild": "npm run clean",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:cjs && npm run build:es",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larriereguichet/universal-react-app.git"
  },
  "keywords": [
    "react",
    "express",
    "server"
  ],
  "author": "lutangar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larriereguichet/universal-react-app/issues"
  },
  "homepage": "https://github.com/larriereguichet/universal-react-app/tree/master/packages/react-express-server#readme",
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "history": "^4.7.2",
    "history-prop-types": "^2.0.4",
    "pino": "^4.10.2",
    "pino-http": "^3.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.4.0",
    "sqwish": "^0.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "babel-preset-lag": "^2.0.4",
    "cross-env": "^5.1.3",
    "rimraf": "^2.6.2"
  }
}
