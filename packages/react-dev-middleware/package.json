  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "babel-preset-lag": "^2.5.0",
